				D__(id__,
						    const_changeDirection_lv) = AUX(id__,
						    const_changeDirection_lv) \ internal_events
				O__(id__,
						    const_changeDirection_lv) = sbisim(diamond(D__(id__,
						    const_changeDirection_lv)))
			endmodule
		
