		      	      	      	  )\{|internal__.TID_Movement_t12,internal__.TID_Movement_t13|})
		      	      	      )
		      	      	) \ {|set_d0,get_d0|}
		      	      )
		      	) \ {|set_d1,get_d1|}
		      )
	
	-- main process
	
	MachineMemorySyncSet = Union({
		union(
			union(
